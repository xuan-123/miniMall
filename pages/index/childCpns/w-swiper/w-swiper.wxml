<!--components/w-swiper/w-swiper.wxml-->
 <swiper class="swiper" indicator-active-color="#ff5777" indicator-dots circular autoplay interval="3000" duration="300" >
  <block wx:for="{{list}}" wx:key="{{index}}">
    <swiper-item class="swiper-item" >
        <image src="{{item.image}}" ></image>
    </swiper-item>
  </block>
    
 </swiper>
 